[
  {
    "key": "ctrl+,",
    "command": "editor.action.triggerSuggest",
    "when": "editorHasCompletionItemProvider && editorTextFocus && !editorReadonly"
  },
  { "key": "ctrl+shift+3", "command": "workbench.view.git" },
  { "key": "ctrl+shift+1", "command": "workbench.view.explorer" },

  { "key": "ctrl+shift+5", "command": "workbench.view.extensions" },
  { "key": "shift+ctrl+4", "command": "workbench.view.debug" },

  {
    "key": "ctrl+shift+2",
    "command": "workbench.view.search",
    "when": "!searchViewletVisible"
  },
  {
    "key": "cmd+k p",
    "command": "extension.sidePreview",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+w",
    "command": "editor.action.smartSelect.shrink",
    "when": "editorTextFocus"
  },
  { "key": "cmd+n", "command": "explorer.newFile" },

  { "key": "cmd+enter", "command": "workbench.action.maximizeEditor" },

  { "key": "cmd+shift+enter", "command": "workbench.action.evenEditorWidths" },

  { "key": "ctrl+tab", "command": "workbench.action.nextEditor" },
  { "key": "ctrl+shift+tab", "command": "workbench.action.previousEditor" },

  {
    "key": "ctrl-p",
    "command": "workbench.action.quickOpen"
  },
  {
    "key": "ctrl+g",
    "command": "editor.emmet.action.expandAbbreviation",
    "when": "editorTextFocus"
  }
]
