"*":
  "tags":
    prefix: "gig-tags"
    body: """
          # Ignore tags created by etags, ctags, gtags (GNU global) and cscope
          TAGS
          !TAGS/
          tags
          !tags/
          gtags.files
          GTAGS
          GRTAGS
          GPATH
          cscope.files
          cscope.out
          cscope.in.out
          cscope.po.out
          """
