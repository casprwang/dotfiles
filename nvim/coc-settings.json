{
  "diagnostic.errorSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "git.changedSign.text": "┃",
  "git.addedSign.text": "┃",
  "git.changeRemovedSign.text": "┃",
  // "git.removedSign.text": "▸",
  "suggest.triggerAfterInsertEnter": true,
  "suggest.timeout": 500,
  "suggest.floatEnable": true,
  "git.addGBlameToVirtualText": false,
  "git.addGBlameToBufferVar": true,
  "diagnostic-languageserver.filetypes": {},
  "tsserver.npm": "/usr/local/opt/node@10/bin/npm",
  "tsserver.log": "verbose",
  "languageserver": {
    "haskell": {
      "command": "hie-wrapper",
      "rootPatterns": ["stack.yaml", "cabal.config", "package.yaml"],
      "filetypes": ["hs", "lhs", "haskell"],
      "initializationOptions": {
        "languageServerHaskell": {
          "hlintOn": true
        }
      }
    }
  },
  "prettier.disableLanguages": ["javascript"],
  "coc.preferences.formatOnSaveFiletypes": [
    // "javascript",
    "json",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx",
    "graphql",
    "css",
    "python",
    "less"
    // "markdown"
  ],
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "typescriptreact": ["typescript"],
    "typescript": ["javascript"]
  },
  "go.enable": true,
  "go.goplsOptions": {
    "completeUnimported": true
  },
  "explorer.position": "right",
  "explorer.icon.enableNerdfont": true,
  "explorer.keyMappings": {
    "<cr>": ["expandable?", "expandOrCollapse", "open"]
  },
  "eslint.filetypes": ["vue", "javascript", "typescript"],
  "eslint.enable": true,
  "eslint.autoFixOnSave": true,
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Args": [
    "--ignore=F401,E501,W391",
    "--max-line-length=88"
  ],
  "python.jediEnabled": false,
  "python.formatting.provider": "autopep8",
  "python.formatting.autopep8Args": ["--max-line-length=88", "--aggressive"]
}
