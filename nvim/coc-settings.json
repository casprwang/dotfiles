{
  "languageserver": {
    "sql": {
      "command": "sqls",
      "args": ["-config", "$HOME/.config/sqls/config.yml"],
      "filetypes": ["sql"],
      "shell": true
    }
  },
  "coc.preferences.jumpCommand": "vsplit",
  "diagnostic.errorSign": "‚è∫",
  "workspace.ignoredFolders": ["$HOME", "$HOME/.cargo/**", "$HOME/.rustup/**"],
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.enableHighlightLineNumber": false,
  "diagnostic.enableMessage": "always",
  "diagnostic.virtualText": false,
  "suggest.triggerAfterInsertEnter": true,
  "suggest.timeout": 5000,
  "suggest.noselect": false,
  "yank.priority": 0,
  "prettier.enable": true,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.useTabs": true,
  "prettier.tabWidth": 2,
  "prettier.arrowParens": "avoid",
  "prettier.trailingComma": "es5",
  "prettier.bracketSpacing": true,
  "sql.formatOptions": { "dialect": "sqlite" },
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "html",
    "scss",
    "javascriptreact",
    "rust",
    "typescript",
    "typescriptreact",
    "python",
    "json",
    "jsonc",
    "css",
    "lua",
    "sql",
    "go",
    "vue"
  ],
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "vue": ["javascript"],
    "typescriptreact": ["typescript"],
    "typescript": ["javascript"]
  },
  "go.enable": true,
  "go.goplsOptions": {
    "completeUnimported": true
  },
  "explorer.previewAction.onHover": "content",
  "explorer.position": "floating",
  "explorer.floating.position": "center",
  "explorer.icon.enableNerdfont": true,
  "explorer.keyMappings.global": {
    "<cr>": ["expandable?", "expandOrCollapse", "open"]
  },
  "Lua.diagnostics.globals": ["vim", "use", "requires", "hs"],
  "Lua.format.enable": true,
  "eslint.runtime": "node",
  "eslint.autoFixOnSave": true,
  "eslint.packageManager": "yarn",
  "eslint.nodePath": ".yarn/sdks",
  "eslint.options": {
    "overrideConfig": {
      "rules": {}
    }
  },
  "python.pythonPath": "python",
  "python.formatting.blackPath": "/usr/local/bin/black",
  "python.formatting.provider": "black",
  "python.formatting.blackArgs": ["--line-length=120"],
  "pyright.disableDiagnostics": false,
  "snippets.ultisnips.pythonPrompt": false
}
