{
  "codeLens.enable": true,
  "tailwindCSS.enable": true,
  "tailwindCSS.includeLanguages": {
    "eelixir": "html",
    "elixir": "html",
    "eruby": "html",
    "html.twig": "html"
  },
  "html.format.templating": true,
  "codeLens.position": "eol",
  "languageserver": {
    "ruby-lsp": {
      "command": "ruby-lsp",
      "initializationOptions": {
        "enabledFeatures": {
          "codeActions": true,
          "codeLens": true,
          "completion": true,
          "definition": true,
          "diagnostics": true,
          "documentHighlights": true,
          "documentLink": true,
          "documentSymbols": true,
          "foldingRanges": true,
          "formatting": true,
          "hover": true,
          "inlayHint": true,
          "onTypeFormatting": true,
          "selectionRanges": true,
          "semanticHighlighting": true,
          "signatureHelp": true,
          "typeHierarchy": true,
          "workspaceSymbol": true
        }
      },
      "filetypes": ["ruby", "erb"],
      "featuresConfiguration": {
        "inlayHint": {
          "implicitHashValue": true,
          "implicitRescue": true
        }
      },
      "indexing": {
        "excludedPatterns": ["path/to/excluded/file.rb"],
        "includedPatterns": ["path/to/included/file.rb"],
        "excludedGems": ["gem1", "gem2", "etc."],
        "excludedMagicComments": ["compiled:true"]
      },
      "formatter": "standard",
      "linters": [],
      "experimentalFeaturesEnabled": true
    }
  },
  "inlayHint.enable": false,
  "inlayHint.enableParameter": false,
  "coc.preferences.jumpCommand": "edit",
  "notification.statusLineProgress": false,
  "diagnostic.errorSign": "‚è∫",
  "workspace.ignoredFolders": ["$HOME", "$HOME/.cargo/**", "$HOME/.rustup/**"],
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.enableHighlightLineNumber": false,
  "diagnostic.enableMessage": "always",
  "diagnostic.virtualText": false,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": false,
  "suggest.triggerAfterInsertEnter": true,
  "suggest.timeout": 5000,
  "suggest.noselect": false,
  "solargraph.diagnostics": false,
  "yank.priority": 90,
  "prettier.enable": true,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.useTabs": true,
  "prettier.tabWidth": 2,
  "zig.enabled": true,
  "zig.startUpMessage": true,
  "zig.path": "zls",
  "zig.debugLog": false,
  "prettier.arrowParens": "avoid",
  "prettier.trailingComma": "es5",
  "prettier.bracketSpacing": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "html",
    "scss",
    "javascriptreact",
    "rust",
    "typescript",
    "typescriptreact",
    "python",
    "astro",
    "json",
    "zig",
    "jsonc",
    "css",
    "lua",
    "sql",
    "go",
    "vue"
  ],
  "go.enable": true,
  "go.goplsOptions": {
    "completeUnimported": true
  },
  "coc.preferences.floatActions": true,
  "Lua.diagnostics.globals": ["vim", "use", "requires", "hs"],
  "Lua.format.enable": true,
  "eslint.runtime": "node",
  "eslint.autoFixOnSave": false,
  "eslint.packageManager": "yarn",
  "eslint.nodePath": ".yarn/sdks",
  "eslint.options": {
    "overrideConfig": {
      "rules": {}
    }
  },
  "python.pythonPath": "python",
  "semanticTokens.enable": true,
  "python.formatting.provider": "ruff",
  "python.analysis.diagnosticSeverityOverrides": {
    "reportAttributeAccessIssue": "none"
  },
  "pyright.disableDiagnostics": false
}
