{
  "suggest.triggerAfterInsertEnter": true,
  "suggest.timeout": 500,
  "suggest.floatEnable": true,
  "git.addGBlameToVirtualText": false,
  "git.addGBlameToBufferVar": true,
  "diagnostic-languageserver.filetypes": {},
  "languageserver": {
    "haskell": {
      "command": "hie-wrapper",
      "rootPatterns": ["stack.yaml", "cabal.config", "package.yaml"],
      "filetypes": ["hs", "lhs", "haskell"],
      "initializationOptions": {
        "languageServerHaskell": {
          "hlintOn": true
        }
      }
    }
  },
  "coc.preferences.formatOnSaveFiletypes": [
    // "javascript",
    // "json",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx",
    "graphql",
    "css",
    "python",
    "less"
    // "markdown"
  ],
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "typescriptreact": ["typescript"],
    "typescript": ["javascript"]
  },
  "go.enable": true,
  "go.goplsOptions": {
    "completeUnimported": true
  },
  "explorer.position": "right",
  "explorer.icon.enableNerdfont": true,
  "explorer.keyMappings": {
    "<cr>": ["expandable?", "expandOrCollapse", "open"]
  },
  "eslint.filetypes": ["vue"],
  "eslint.enable": true,
  "eslint.autoFixOnSave": false,
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Args": ["--ignore=F401,E501,W391", "--max-line-length=88"],
  "python.jediEnabled": false,
  "python.formatting.provider": "autopep8",
  "python.formatting.autopep8Args": ["--max-line-length=88", "--aggressive"]
  // "python.linting.enabled": false,
  // "python.analysis.diagnosticEnabled": false
}
